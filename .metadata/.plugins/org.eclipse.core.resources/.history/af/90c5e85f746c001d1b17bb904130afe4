package com.jspider.hibernateonetoone.dto;

import javax.persistence.Entity;
import javax.persistence.Id;

import lombok.Data;

@Data
@Entity
public class WifeDTO {
	
	@Id
	private int id;
	
	private String name;
	
	private String email;
	
	private int age;
	
	private long contact;

	public WifeDTO() {
		super();
		// TODO Auto-generated constructor stub
	}

	public WifeDTO(int id, String name, String email, int age, long contact) {
		super();
		this.id = id;
		this.name = name;
		this.email = email;
		this.age = age;
		this.contact = contact;
	}
	
	
}
